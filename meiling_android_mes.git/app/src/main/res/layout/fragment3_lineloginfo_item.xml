<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="horizontal"
    >

    <TextView
        android:id="@+id/fragment3_lineloginfo_starttime"
        style="@style/textview_indent_style1"
        android:text="开始时间" />

    <TextView
        android:id="@+id/fragment3_lineloginfo_endtime"
        style="@style/textview_indent_style1"
        android:text="结束时间" />

    <TextView
        android:id="@+id/fragment3_lineloginfo_usercode"
        style="@style/textview_indent_style1"
        android:text="操作员编码" />

    <TextView
        android:id="@+id/fragment3_lineloginfo_username"
        style="@style/textview_indent_style1"
        android:text="操作员名称" />

    <TextView
        android:id="@+id/fragment3_lineloginfo_note"
        style="@style/textview_indent_style1"
        android:layout_weight="2"
        android:text="备注" />
</LinearLayout>