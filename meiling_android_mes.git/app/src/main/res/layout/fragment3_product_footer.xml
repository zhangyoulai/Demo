<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">
    <!--
        订单号，计划总数量、预装，（总装、在生产数量），（交库，已完成数量），完成率  ，交货时间，交货预警，订单日计划数据-->

    <TextView
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/footer_title_text_color"/>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <TextView
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="@color/footer_title_text_color"/>
        <TextView
            android:id="@+id/fragment3_footer_textview6"
            style="@style/textview_indent_style2"
            android:text="合"
            android:gravity="center|right"/>
        <TextView
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="@color/white"/>
        <TextView
            android:id="@+id/fragment3_footer_textview9"
            style="@style/textview_indent_style2"
            android:text="计"
            android:gravity="center|left"/>
        <TextView
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="@color/footer_title_text_color"/>
        <TextView
            android:id="@+id/fragment3_item_textview2"
            style="@style/textview_indent_style2"
            android:visibility="gone"
            android:text="计划数量" />
        <TextView
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:visibility="gone"
            android:background="@color/footer_title_text_color"/>
        <TextView
            android:id="@+id/fragment3_footer_textview3"
            style="@style/textview_indent_style2"
            android:text="0" />
        <TextView
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="@color/footer_title_text_color"/>
        <TextView
            android:id="@+id/fragment3_footer_textview7"
            style="@style/textview_indent_style2"
            android:text="0" />
        <TextView
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="@color/footer_title_text_color"/>
        <TextView
            android:id="@+id/fragment3_footer_textview8"
            style="@style/textview_indent_style2"
            android:text="0" />
        <TextView
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="@color/footer_title_text_color"/>
        <TextView
            android:id="@+id/fragment3_footer_textview4"
            style="@style/textview_indent_style2"
            android:text="0" />
        <TextView
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="@color/footer_title_text_color"/>
        <TextView
            android:id="@+id/fragment3_footer_textview5"
            style="@style/textview_indent_style2"
            android:text="0" />
        <TextView
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="@color/footer_title_text_color"/>
        <TextView
            style="@style/textview_indent_style2"
            android:visibility="gone"
            android:text="0" />
        <TextView
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:visibility="gone"
            android:background="@color/footer_title_text_color"/>
        <ProgressBar
            android:visibility="gone"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="wrap_content"
            android:padding="10dp"
            style="@style/StyleProgressBarMini"
            android:layout_gravity="center_vertical"
            android:text="0" />
        <TextView
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:visibility="gone"
            android:background="@color/footer_title_text_color"/>
    </LinearLayout>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/footer_title_text_color"/>
</LinearLayout>